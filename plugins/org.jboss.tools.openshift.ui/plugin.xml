<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension
		point="org.eclipse.ui.navigator.viewer">
		<viewerContentBinding
			viewerId="org.jboss.tools.openshift.express.ui.explorer.expressConsoleView">
			<includes>
				<contentExtension
					isRoot="true"
					pattern="org.jboss.tools.openshift.ui.explorer.navigatorContent">
				</contentExtension>
			</includes>
		</viewerContentBinding>
	</extension>
	<extension
		point="org.eclipse.ui.navigator.navigatorContent">
		<navigatorContent
			contentProvider="org.jboss.tools.openshift.internal.ui.explorer.OpenShiftExplorerContentProvider"
			id="org.jboss.tools.openshift.ui.explorer.navigatorContent"
			labelProvider="org.jboss.tools.openshift.internal.ui.explorer.OpenShiftExplorerLabelProvider"
			name="OpenShift 3 Explorer Content">
			<possibleChildren>
				<or>
					<instanceof
						value="com.openshift.restclient.model.IResource" />
					<instanceof
						value="org.jboss.tools.openshift.internal.ui.explorer.ResourceGrouping" />
				</or>
			</possibleChildren>
			<triggerPoints>
				<or>
					<instanceof
						value="org.jboss.tools.openshift.core.connection.Connection" />
					<instanceof
						value="com.openshift.restclient.model.IProject" />
					<instanceof
						value="org.jboss.tools.openshift.internal.ui.explorer.ResourceGrouping" />
				</or>
			</triggerPoints>
		</navigatorContent>
	</extension>
	<extension
		point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.import">
			<command
				commandId="org.jboss.tools.openshift.ui.command.importapplication"
				icon="icons/application_import.gif"
				label="%openshift.command.application.import"
				mnemonic="A">
				<visibleWhen>
					<with
						variable="selection">
						<count
							value="1">
						</count>
						<iterate
							ifEmpty="false">
							<or>
								<instanceof
									value="org.jboss.tools.openshift.core.connection.Connection" />
								<instanceof
									value="com.openshift.restclient.model.IBuildConfig" />
								<instanceof
									value="com.openshift.restclient.model.IProject" />
							</or>
						</iterate>
					</with>
				</visibleWhen>
			</command>
			<command
				commandId="org.jboss.tools.openshift.ui.command.deployimage"
				icon="icons/layer.png"
				label="%openshift.command.application.deploydockerimage "
				style="push"
				tooltip="Deploy a Docker Image">
				<visibleWhen
					checkEnabled="false">
					<and>
						<count
							value="1">
						</count>
						<iterate>
							<or>
								<instanceof
									value="com.openshift.restclient.model.IProject" />
								<instanceof
									value="org.jboss.tools.openshift.core.connection.Connection" />
							</or>
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
		
		<!-- openshift explorer context menu: refresh resource -->
		<menuContribution
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
			<command
				commandId="org.jboss.tools.openshift.ui.command.resource.refresh"
				icon="icons/refresh.gif"
				id="org.jboss.tools.openshift.ui.command.resource.refresh"
				style="push"
				tooltip="Refresh the resource">
				<visibleWhen>
					<and>
						<count
							value="1" />
						<iterate>
							<or>
								<instanceof
									value="org.jboss.tools.openshift.common.core.IRefreshable" />
								<instanceof
									value="com.openshift.restclient.model.IResource" />
								<instanceof
                  					value="com.openshift.restclient.OpenShiftException" />
							</or>
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>

		<!-- openshift explorer context menu: new connection -->
		<menuContribution
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.edit">
			<command
				commandId="org.eclipse.ui.edit.delete"
				id="org.jboss.tools.openshift.ui.command.resource.delete"
				style="push"
				tooltip="Delete the resource">
				<visibleWhen>
					<and>
						<count
							value="1">
						</count>
						<or>
						<iterate>
							<and>
								<instanceof
									value="com.openshift.restclient.model.IResource" />
								<not>
									<instanceof
										value="com.openshift.restclient.model.IProject" />
								</not>
							</and>
						</iterate>
						</or>
					</and>
				</visibleWhen>
			</command>
			<separator
				name="org.jboss.tools.openshift.ui.delete.separator"
					visible="true">
			</separator>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleViewPopup.new?after=additions">
			<command
				commandId="org.jboss.tools.openshift.ui.command.project.new"
				style="push"
				icon="icons/globe_new.png"
				tooltip="Create a new project">
				<visibleWhen
					checkEnabled="false">
					<and>
						<count
							value="1" />
						<iterate>
						 <or>
							<instanceof
								value="com.openshift.restclient.model.IProject" />
							<instanceof
                    			value="org.jboss.tools.openshift.core.connection.Connection"/>
						 </or>
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleViewPopup.new?after=additions">
			<command
				commandId="org.jboss.tools.openshift.ui.command.application.new"
				style="push"
				icon="icons/query-new.gif"
				tooltip="Create a new application">
				<visibleWhen
					checkEnabled="false">
					<and>
						<count
							value="1" />
						<iterate>
						 <or>
							<instanceof
								value="com.openshift.restclient.model.IProject" />
							<instanceof
                    			value="org.jboss.tools.openshift.core.connection.Connection"/>
						 </or>
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
		   <command
		         commandId="org.jboss.tools.openshift.ui.command.startbuild"
		         id="org.jboss.tools.openshift.ui.explorer.command.startbuild">
		      <visibleWhen>
		         <with
		               variable="selection">
		            <and>
		               <count
		                     value="1">
		               </count>
		               <iterate
		                     ifEmpty="false"
		                     operator="and">
		                  <instanceof
		                        value="com.openshift.restclient.model.IBuildConfig">
		                  </instanceof>
		               </iterate>
		            </and>
		         </with>
		      </visibleWhen>
		   </command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
		   <command
		         commandId="org.jboss.tools.openshift.ui.command.editresource"
		         id="org.jboss.tools.openshift.ui.explorer.command.editresource">
		      <visibleWhen>
		         <with
		               variable="selection">
		            <and>
		               <count
		                     value="1">
		               </count>
		               <iterate
		                     ifEmpty="false">
		                  <or>
		                  <instanceof
		                        value="com.openshift.restclient.model.IBuildConfig">
		                  </instanceof>
		                  <instanceof
		                        value="com.openshift.restclient.model.IDeploymentConfig">
		                  </instanceof>
		                  <instanceof
		                        value="com.openshift.restclient.model.IPod">
		                  </instanceof>
		                  <instanceof
		                        value="com.openshift.restclient.model.IImageStream">
		                  </instanceof>
		                  <instanceof
		                        value="com.openshift.restclient.model.IReplicationController">
		                  </instanceof>
		                  <instanceof
		                        value="com.openshift.restclient.model.IService">
		                  </instanceof>
		                  <instanceof
		                        value="com.openshift.restclient.model.route.IRoute">
		                  </instanceof>
		                  </or>
		               </iterate>
		            </and>
		         </with>
		      </visibleWhen>
		   </command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
		   <command
		         commandId="org.jboss.tools.openshift.ui.command.clonebuild"
		         id="org.jboss.tools.openshift.ui.explorer.command.clonebuild">
		      <visibleWhen>
		         <with
		               variable="selection">
		            <and>
		               <count
		                     value="1">
		               </count>
		               <iterate
		                     ifEmpty="false"
		                     operator="and">
		                  <instanceof
		                        value="com.openshift.restclient.model.IBuild">
		                  </instanceof>
		               </iterate>
		            </and>
		         </with>
		      </visibleWhen>
		   </command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
		   <command
		         commandId="org.jboss.tools.openshift.ui.command.showwebhooks"
		         id="org.jboss.tools.openshift.ui.explorer.command.showwebhooks">
		      <visibleWhen>
		         <with
		               variable="selection">
		            <and>
		               <count
		                     value="1">
		               </count>
		               <iterate
		                     ifEmpty="false"
		                     operator="and">
		                  <instanceof
		                        value="com.openshift.restclient.model.IBuildConfig">
		                  </instanceof>
		               </iterate>
		            </and>
		         </with>
		      </visibleWhen>
		   </command>
		</menuContribution>
 		<menuContribution
        	allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.showIn">
			<command
				commandId="org.jboss.tools.openshift.ui.command.project.manage"
				style="push"
				tooltip="Manage Projects">
				<visibleWhen
					checkEnabled="false">
					<and>
						<count
							value="1">
						</count>
						<iterate>
							<or>
								<instanceof
									value="org.jboss.tools.openshift.core.connection.Connection"/>
							</or>
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
        	locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.showIn">
			<command
				commandId="org.eclipse.ui.edit.delete"
				style="push"
				icon="icons/globe_delete.png"
				tooltip="Delete Project">
				<visibleWhen
					checkEnabled="false">
					<and>
						<!-- TODO support deletion of several projects in 1 command -->
						<count
                    		value="1">
						</count>
						<or>
						<iterate>
							<instanceof
								value="com.openshift.restclient.model.IProject">
							</instanceof>
						</iterate>
						</or>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
			<command
				commandId="org.jboss.tools.openshift.ui.command.portforwarding"
				id="org.jboss.tools.openshift.ui.explorer.command.portforwarding"
				tooltip="The openshift binary must be set in user preferences in order to use port forwarding">
				<visibleWhen>
					<with
						variable="selection">
						<and>
							<count
								value="1">
							</count>
		                	<iterate
								ifEmpty="false"
								operator="and">
								<instanceof
									value="com.openshift.restclient.model.IPod" />
							</iterate>
						</and>
					</with>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
			<command
				commandId="org.jboss.tools.openshift.ui.command.podlogs"
				id="org.jboss.tools.openshift.ui.explorer.command.podlogs"
				tooltip="The openshift binary must be set in user preferences in order to retrieve pod logs">
				<visibleWhen>
					<with
						variable="selection">
						<and>
							<count
								value="1">
							</count>
							<iterate
								ifEmpty="false"
								operator="and">
								<instanceof
									value="com.openshift.restclient.model.IPod">
								</instanceof>
							</iterate>
						</and>
					</with>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.modify">
			<command
				commandId="org.jboss.tools.openshift.ui.command.buildlogs"
				id="org.jboss.tools.openshift.ui.explorer.command.buildlogs"
				tooltip="The openshift binary must be set in user preferences in order to retrieve build logs">
				<visibleWhen>
					<with
						variable="selection">
						<and>
							<count
								value="1">
							</count>
							<iterate
								ifEmpty="false"
								operator="and">
                    			<instanceof
									value="com.openshift.restclient.model.IBuild">
								</instanceof>
							</iterate>
						</and>
					</with>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.jboss.tools.openshift.express.ui.explorer.expressConsoleView.popup?after=group.properties">
			<command
				commandId="org.eclipse.ui.file.properties"
				id="org.jboss.tools.openshift.ui.explorer.command.showproperties">
				<visibleWhen>
					<with
						variable="selection">
						<and>
							<count
								value="1">
							</count>
							<iterate
								ifEmpty="false">
								<or>
									<instanceof
										value="com.openshift.restclient.model.IResource">
									</instanceof>
									<instanceof
										value="org.jboss.tools.openshift.core.connection.Connection">
									</instanceof>
								</or>
							</iterate>
						</and>
					</with>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
			allPopups="false"
			locationURI="popup:org.eclipse.linuxtools.docker.ui.dockerExplorerView#PopupMenu">
			<separator
				name="org.jboss.tools.openshift.ui.separator.docker.explorer"
				visible="true">
			</separator>
			<command
				commandId="org.jboss.tools.openshift.ui.command.deployimage"
				icon="icons/openshift-logo-white-icon.png"
				id="org.jboss.tools.openshift.ui.docker.explorer.deploy"
				style="push">
				<visibleWhen
					checkEnabled="false">
					<with
						variable="selection">
						<and>
							<count
								value="1">
							</count>
							<iterate
								ifEmpty="false"
								operator="and">
								<instanceof
									value="org.eclipse.linuxtools.docker.core.IDockerImage">
								</instanceof>
							</iterate>
						</and>
					</with>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.ManageProjectsHandler"
			commandId="org.jboss.tools.openshift.ui.command.project.manage">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.DeleteProjectsHandler"
			commandId="org.eclipse.ui.edit.delete">
				<activeWhen>
					<and>
						<count
							value="1">
						</count>
						<iterate>
							<instanceof
								value="com.openshift.restclient.model.IProject">
							</instanceof>
						</iterate>
					</and>
				</activeWhen>
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.NewProjectHandler"
			commandId="org.jboss.tools.openshift.ui.command.project.new">
		</handler>
  		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.NewApplicationHandler"
			commandId="org.jboss.tools.openshift.ui.command.application.new">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.DeleteResourceHandler"
			commandId="org.eclipse.ui.edit.delete">
				<activeWhen>
					<and>
						<count
							value="1">
						</count>
						<iterate>
							<and>
								<instanceof
									value="com.openshift.restclient.model.IResource" />
								<not>
									<instanceof
										value="com.openshift.restclient.model.IProject" />
								</not>
							</and>
						</iterate>
					</and>
				</activeWhen>
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.RefreshResourceHandler"
			commandId="org.jboss.tools.openshift.ui.command.resource.refresh">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.PortForwardingHandler"
			commandId="org.jboss.tools.openshift.ui.command.portforwarding">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.StartBuildHandler"
			commandId="org.jboss.tools.openshift.ui.command.startbuild">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.StartBuildHandler"
			commandId="org.jboss.tools.openshift.ui.command.clonebuild">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.PodLogsHandler"
			commandId="org.jboss.tools.openshift.ui.command.podlogs">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.ShowWebHooksHandler"
			commandId="org.jboss.tools.openshift.ui.command.showwebhooks">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.ShowPropertiesHandler"
			commandId="org.eclipse.ui.file.properties">
				<activeWhen>
					<with
						variable="selection">
						<and>
							<count
								value="1">
							</count>
							<iterate
								ifEmpty="false">
								<or>
									<instanceof
										value="com.openshift.restclient.model.IResource">
									</instanceof>
									<instanceof
										value="org.jboss.tools.openshift.core.connection.Connection">
									</instanceof>
								</or>
							</iterate>
						</and>
					</with>
				</activeWhen>
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.DeployImageHandler"
			commandId="org.jboss.tools.openshift.ui.command.deployimage">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.OpenInWebConsoleHandler"
			commandId="org.jboss.tools.openshift.ui.command.openInWebConsole">
		</handler>
		<handler
			class="org.jboss.tools.openshift.internal.ui.handler.EditResourceHandler"
			commandId="org.jboss.tools.openshift.ui.command.editresource">
		</handler>
	</extension>
	
	<!-- adapters -->
	<extension
		point="org.eclipse.core.runtime.adapters">
		<factory
			adaptableType="com.openshift.restclient.model.IResource"
			class="org.jboss.tools.openshift.internal.ui.property.OpenShiftPropertySourceAdapterFactory">
			<adapter
				type="org.eclipse.ui.views.properties.IPropertySource">
		</adapter>
	</factory>
	<factory
		adaptableType="org.jboss.tools.openshift.core.connection.Connection"
		class="org.jboss.tools.openshift.internal.ui.property.OpenShiftPropertySourceAdapterFactory">
		<adapter
			type="org.eclipse.ui.views.properties.IPropertySource">
		</adapter>
	</factory>
	<factory
		adaptableType="org.eclipse.wst.server.core.IServer"
		class="org.jboss.tools.openshift.internal.ui.server.OpenShiftServerAdapterFactory">
		<adapter
			type="org.eclipse.wst.server.ui.IServerModule">
		</adapter>
	</factory>
	</extension>

	<!-- definitions -->
	<extension
		point="org.eclipse.core.expressions.definitions">
		<definition
			id="org.tools.openshift.express.ui.isOpenShiftProjectSelected">
			<with
				variable="selection">
				<iterate
					ifEmpty="false"
					operator="and">
					<count
						value="1" />
					<or>
						<instanceof
							value="com.openshift.restclient.model.IProject"/>
					</or>
				</iterate>
			</with>
		</definition>
	</extension>
	<extension
       point="org.eclipse.ui.commands">
		<command
			description="Create a new application from a template"
			id="org.jboss.tools.openshift.ui.command.application.new"
			name="Application">
		</command>
		<command
			description="Create a new project"
			id="org.jboss.tools.openshift.ui.command.project.new"
			name="Project">
		</command>
		<command
			description="Delete the Resource"
			id="org.jboss.tools.openshift.ui.command.resource.delete"
			name="Delete Resource...">
		</command>
		<command
			description="Manage Projects"
			id="org.jboss.tools.openshift.ui.command.project.manage"
			name="%openshift.command.project.manage">
		</command>
		<command
			description="Delete Project"
			id="org.jboss.tools.openshift.ui.command.project.delete"
			name="%openshift.command.project.delete">
		</command>
		<command
			description="Refresh the Resource"
			id="org.jboss.tools.openshift.ui.command.resource.refresh"
			name="Refresh">
		</command>
		<command
			id="org.jboss.tools.openshift.ui.command.portforwarding"
			name="%openshift.command.application.portforwarding">
		</command>
		<command
			id="org.jboss.tools.openshift.ui.command.startbuild"
			name="%openshift.command.build.startbuild">
		</command>
		<command
			id="org.jboss.tools.openshift.ui.command.clonebuild"
			name="%openshift.command.build.clonebuild">
		</command>
		<command
			id="org.jboss.tools.openshift.ui.command.podlogs"
			name="%openshift.command.pod.logs">
		</command>
		<command
			defaultHandler="org.jboss.tools.openshift.internal.ui.handler.PodLogsHandler"
			id="org.jboss.tools.openshift.ui.command.buildlogs"
